rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /Users/{documentID}{
    	allow read,write,update,delete : if request.auth.uid == documentID;
    	allow create : if request.auth.uid != null;
    }
    
    match /Rank/{documentID}{
    allow read, create, write, update : if request.auth.uid != null;
    allow delete : if false;
    }
  }
}